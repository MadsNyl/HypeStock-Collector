CREATE TABLE sentiment (
    id int AUTO_INCREMENT,
    symbol varchar(5),
    is_reddit boolean,
    post_url varchar(255),
    permalink varchar(255),
    created_date datetime,
    body text,
    author varchar(255),
    negative_score float,
    neutral_score float,
    positive_score float,
    PRIMARY KEY (id),
    FOREIGN KEY (symbol) REFERENCES stock (symbol)
)